<launch>
  <!-- 声明启动参数 -->
  <arg name="config_file" default="$(find walk_motor_driver)/config/walk_motor_params.yaml"
       doc="参数配置文件路径"/>

  <!-- 行走电机驱动节点 -->
  <node pkg="walk_motor_driver" type="walk_motor_driver_node" name="walk_motor_driver" output="screen">
    <!-- 从YAML文件加载所有参数 -->
    <rosparam command="load" file="$(arg config_file)"/>
  </node>
</launch>
