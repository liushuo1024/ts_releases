        <param name="publish_frequency" type="double" value="50.0" />
    </node>-->

  <!-- servo node -->
  <node pkg="sim_control" type="servo_commands.py" name="servo_commands" output="screen">
  </node>
  <!-- <node pkg="sim_control" type="ac_node.py" name="ac_node" output="screen">
  </node> -->

  <node pkg="sim_control" name="sim_encoder_node" type="sim_encoder_node.py">
        <param name="pub_hz"   value="20" />
  </node>


    
</launch>
